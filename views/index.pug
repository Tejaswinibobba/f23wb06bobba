extends layout

block content
  h1= title
  p Welcome to #{title}
  p welcome to Tejaswini Bobba page
  button.btn.btn-outline-primary(type="button",onclick="goFaster()") press me
  div#faster Increase the speed.
  script.
    var mph = 20;
    var change = 1;

    function goFaster() {
      mph += change;
      change += 1;
      var div = document.getElementById("faster");
      div.innerText = "MPH " + mph + " with change " + change;
    }
    


